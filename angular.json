{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "fb7a2661-a357-4439-8fd0-208bc355cccd"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-page": {
      "projectType": "library",
      "root": "projects/agencycoda/mia-page",
      "sourceRoot": "projects/agencycoda/mia-page/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/agencycoda/mia-page/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-page/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/agencycoda/mia-page/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-page/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-page/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-page/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "@agencycoda/mia-page"
}
